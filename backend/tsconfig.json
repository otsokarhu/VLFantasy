{
  "compilerOptions": {
    "lib": [
      "ES2022"
    ],
    "target": "ES2022",
    "outDir": "./build/",
    "module": "CommonJS",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
  }
}